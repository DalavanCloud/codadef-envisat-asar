<?xml version="1.0"?>
<cd:Record format="binary" name="ADSR_IM_Doppler_Centroid_Grid" last-modified="2018-07-11" xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07">
  <cd:Field name="first_zero_doppler_time">
    <cd:Time timeformat="binary_envisat_datetime">
      <cd:Description>First Zero Doppler Azimuth time of raw signal data used for this estimates</cd:Description>
    </cd:Time>
  </cd:Field>
  <cd:Field name="attach_flag">
    <cd:Integer>
      <cd:Description>Attachment Flag (always set to zero for this ADSR)</cd:Description>
      <cd:BitSize>8</cd:BitSize>
      <cd:NativeType>uint8</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="slant_range_time">
    <cd:Array>
      <cd:Description>2-way slant range time origin (t0)</cd:Description>
      <cd:Dimension>100</cd:Dimension>
      <cd:Float>
        <cd:Unit>ns</cd:Unit>
        <cd:BitSize>32</cd:BitSize>
        <cd:NativeType>float</cd:NativeType>
      </cd:Float>
    </cd:Array>
  </cd:Field>
  <cd:Field name="dop_coef">
    <cd:Array>
      <cd:Description>Fine Doppler centroid frequency estimate. This is the original estimated Doppler centroid frequncy befor any polynomial fitting is applied</cd:Description>
      <cd:Dimension>100</cd:Dimension>
      <cd:Float>
        <cd:Unit>Hz</cd:Unit>
        <cd:BitSize>32</cd:BitSize>
        <cd:NativeType>float</cd:NativeType>
      </cd:Float>
    </cd:Array>
  </cd:Field>
  <cd:Field name="last_zero_doppler_time">
    <cd:Time timeformat="binary_envisat_datetime">
      <cd:Description>Last Zero Doppler Azimuth time of raw signal data used for this estimates</cd:Description>
    </cd:Time>
  </cd:Field>
  <cd:Field name="spare_1">
    <cd:Raw>
      <cd:Description>Spare</cd:Description>
      <cd:BitSize>388*8</cd:BitSize>
    </cd:Raw>
    <cd:Hidden/>
  </cd:Field>
</cd:Record>
